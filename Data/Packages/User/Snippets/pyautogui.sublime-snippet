from pyautogui import *
def h(a):
	v=(('#','win+'),
	('^','ctrl+'),	
	('+','shift+'),	
	('!','alt+'),)
	('`,'tab+'),)
	for i,j in v:a=a.replace(i,j)
	hotkey(*a.strip('+').split('+'))


